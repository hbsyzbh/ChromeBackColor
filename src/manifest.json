{
	"name": "BackColor",
	"version": "1.0.0",
	"description": "BackColor",
	"manifest_version": 2,
	"icons": {
	"16": "19.png"
	},
	"browser_action": {
		"default_icon": "19.png",
		"default_popup": "popup.html",
		"default_title": "BackColor"
	},
	"background": {
		"page": "background.html"
	},
	"content_scripts": [
    {
	  "matches": [ "<all_urls>" ],
      "js": ["jquery.min.js", "color.js"]
    }
	]
}